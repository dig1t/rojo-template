{
  "name": "rojo-template",
  "globIgnorePaths": ["**/*.spec.luau"],

  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",

      "Packages": {
        "$path": "Packages"
      },

      "Shared": {
        "$path": "src/Shared"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$path": "src/Server"
    },

    "ReplicatedFirst": {
      "$className": "ReplicatedFirst",
      "$path": "src/Client"
    }
  }
}
